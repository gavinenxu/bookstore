type Book {
  id: ID! @unique
  primary_isbn: String!
  title: String!
  authors: [Author!]!
  bookkey: String
  isbns: [String]
  price: String
  amazon_rank: Int
  publisher: String
  primary_bisacs: [String]
  description: String
  active: Boolean
  image: String
}

type Author {
  id: ID! @unique
  role: Int
  firebrand_role: String
  short_bio: String
  firebrand_id: Int
  display_name: String
  first_name: String
  last_name: String
  slug: String
}
